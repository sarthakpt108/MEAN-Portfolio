{
  "name": "normalize-css-unit",
  "version": "1.0.1",
  "description": "Normalize CSS Unit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fpoumian/normalize-css-unit.git"
  },
  "author": {
    "name": "Fernando PoumiÃ¡n",
    "email": "fpoumian@gmail.com",
    "url": "https://github.com/fpoumian"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "scripts": {
    "start": "nps --no-scripts",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "lint-staged && flow --quiet",
    "test": "nps test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "eslint --fix",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "lodash.isnumber": "3.0.3",
    "lodash.isstring": "4.0.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.4.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.3.0",
    "eslint-config-bunny": "^4.0.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "flow-bin": "^0.51.0",
    "flow-typed": "^2.1.5",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.2",
    "nps": "^5.7.0",
    "nps-utils": "^1.3.0",
    "opt-cli": "^1.5.1",
    "prettier": "^1.5.3",
    "validate-commit-msg": "^2.14.0",
    "semantic-release": "^8.0.3"
  },
  "keywords": [
    "css",
    "unit",
    "validate",
    "pixels",
    "px",
    "rem",
    "em",
    "length"
  ]
}
